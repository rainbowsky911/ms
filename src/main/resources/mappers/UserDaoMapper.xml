<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zitai.ms.dao.UserDao">

    <!--根据秒杀商品id查询库存-->
    <select id="selectByPrimaryKey" parameterType="int" resultType="com.zitai.ms.entity.User">
        select id,user_name from user
        where id = #{id}
    </select>


</mapper>